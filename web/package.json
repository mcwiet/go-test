{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "env-cmd -f ../.env react-scripts start",
    "build": "env-cmd -f ../.env react-scripts build",
    "test": "env-cmd -f ../.env react-scripts test --watchAll=false --silent --coverage --coverageReporters='text-summary'",
    "test-save-coverage": "env-cmd -f ../.env react-scripts test --watchAll=false --silent --coverage --coverageReporters='lcov'",
    "clean": "rm -rf build node_modules",
    "format": "npx prettier --write src/**/*.ts src/**/*.tsx src/**/*.css"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@mui/core": "^5.0.0-alpha.54",
    "@mui/icons-material": "^5.4.4",
    "@mui/material": "^5.4.4",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "aws-amplify": "^4.3.15",
    "env-cmd": "^10.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "react-scripts": "^5.0.0",
    "typescript": "^4.6.2",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "prettier": "^2.5.1"
  }
}
